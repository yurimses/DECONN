TIMEOUT(259200000, log.log("last msg: " + msg + "\n")); /* milliseconds. print last msg at timeout */

sim.setSpeedLimit(9.99);
while(sim.getSimulationTimeMillis() &lt;= 259200000){

log.log((sim.getSimulationTimeMillis()/1000) + " : " + id + " mote output: '" + msg + "'\n");

YIELD(); /* wait for another mote output */

}
log.testOK(); /* Report test success and quit */</script>
